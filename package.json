{
	"name": "belgiumjs.github.io",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "npm run test:unit && npm run test:storybook",
		"test:unit": "jest",
		"test:storybook": "concurrently -P -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook -- --ci\" \"npx wait-on http://localhost:6006 && npx test-storybook {@}\"",
		"test:storybook:snapshot": "npm run test:storybook -- -- --updateSnapshot",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build"
	},
	"dependencies": {
		"@storybook/test-runner": "^0.11.0",
		"concurrently": "^8.2.0",
		"next": "13.4.7",
		"next-mdx-remote": "^4.4.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.9.0",
		"react-intl": "^6.4.4",
		"sass": "^1.63.6",
		"turbo": "^1.10.6",
		"typescript": "5.1.3"
	},
	"devDependencies": {
		"@storybook/addon-controls": "^7.0.24",
		"@storybook/addon-essentials": "^7.0.24",
		"@storybook/addon-interactions": "^7.0.24",
		"@storybook/addon-links": "^7.0.24",
		"@storybook/addon-viewport": "^7.0.24",
		"@storybook/blocks": "^7.0.24",
		"@storybook/nextjs": "^7.0.24",
		"@storybook/react": "^7.0.24",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@types/node": "20.3.1",
		"@types/react": "18.2.11",
		"@types/react-dom": "18.2.6",
		"eslint": "8.43.0",
		"eslint-config-next": "13.4.7",
		"eslint-plugin-storybook": "^0.6.12",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"prettier": "2.8.8",
		"storybook": "^7.0.24"
	}
}
