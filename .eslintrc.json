{
	"extends": "next/core-web-vitals",
	"rules": {
		"no-console": "error",
		"quotes": ["error", "single"],
		"no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
		"eol-last": ["error", "always"],
		"no-trailing-spaces": "error",
		"semi": ["error", "always"],
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
	}
}
